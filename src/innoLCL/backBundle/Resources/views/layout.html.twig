        <script src="js/vendor/modernizr-2.8.3-respond-1.4.2.min.js"></script>-->
    </head>
    <body>
        <nav class="navbar navbar-default navbar-static-top" role="navigation">
            <div class="">
                <div class="col-md-4 text-right">
                    {% image '@innoLCLbackBundle/Resources/assets/images/logo_LCL.png' %}
                                <img src="{{ asset_url }}"  alt="LCL" style="vertical-align: middle;" />
                     {% endimage %} 
                     <span class="nav_challenge">Le Challenge des idées</span>
                </div>
                <div class="col-md-4  text-center">
                    {% image '@innoLCLbackBundle/Resources/assets/images/logo-challenge-bo.png' %}
                                <img src="{{ asset_url }}"  alt="logo challenge" />
                     {% endimage %} 
                </div>
                <div class="col-md-4  text-right" style="padding-top:1.5em;">
                    <a href="{{ path('fos_user_security_logout') }}" class="logout">Déconnexion</a> 
                </div>                     
            </div>
        </nav>
        
    {% block menu  %}
    {% endblock %}
    
    {% block enteteSelectionneur  %}
    {% endblock %}
    
    {% block body  %}
    {% endblock %}
    
    <footer class="row">
       
    </footer>
        {% javascripts
        '@innoLCLbackBundle/Resources/assets/js/jquery-2.1.4.min.js'
        '@innoLCLbackBundle/Resources/assets/js/admin.js'
        output='js/compiled/app.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
