        <meta name="author" content="Challenge de l'innovation LCL">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="apple-touch-icon" href="apple-touch-icon.png">
<!--filter='cssrewrite'-->
        {% stylesheets 
            '@innoLCLbackBundle/Resources/assets/css/bootstrap.min.css' 
            '@innoLCLbackBundle/Resources/assets/css/sb-admin-2.css'             
            '@innoLCLbackBundle/Resources/assets/css/theme.css'             
            %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}
        <!--<link rel="stylesheet" href="css/normalize.min.css">
