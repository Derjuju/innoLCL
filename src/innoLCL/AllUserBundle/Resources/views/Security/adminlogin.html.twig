          <script src="//cdnjs.cloudflare.com/ajax/libs/respond.js/1.1.0/respond.min.js"></script> 
          <script src="{{ asset('js/rem.min.js') }}"></script>
        <![endif]--> 
        
<!--filter='cssrewrite'-->
        {% stylesheets 
            '@innoLCLbackBundle/Resources/assets/css/bootstrap.min.css' 
            '@innoLCLbackBundle/Resources/assets/css/sb-admin-2.css'             
            '@innoLCLbackBundle/Resources/assets/css/theme.css'             
            %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}
        
        <!--[if lte IE 8]>  
        {% stylesheets '/css/*' filter='cssrewrite'      
            '@innoLCLbackBundle/Resources/assets/css/back-ie8-fix.css'   
            %}
            <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}
         <![endif]-->         


    </head>
    <body>
        <nav class="navbar navbar-default navbar-static-top navbar-admin" role="navigation">
			<div class="navContent">
				<div class="col-sm-6 col-md-4 text-right">
					<a href="{{path('innolcl_front_homepage')}}" class="navLink"><img src="{{asset('images/516747b.png')}}" alt="LCL" style="vertical-align: middle;">
					<span class="nav_challenge">Le Challenge des id√©es</span>
					</a>
				</div>
				<div class="col-sm-5 col-md-4  text-center">
					<img src="{{asset('images/8bbf6ff.png')}}" alt="logo challenge">
				</div>
				<div class="col-sm-1 col-md-4  text-right" style="padding-top:1.5em;">

                </div>                     
			</div>
			<div class="clear"></div>
		</nav>
            
<div class="admin-auth">
     <div class="col-md-7">
     
     
     </div>
     <div class="col-md-4 col-lg-3 ie-admin-login">
         <div class="row">
                {% for type, messages in app.session.flashBag.all %}
                {% for message in messages %}
                <div class="{{ type }} erreur-connexion">
                {{ message|trans({}, 'FOSUserBundle') }}
                </div>
                {% endfor %}
                {% endfor %}
                {% if error %}
                <div class="erreur-connexion">{{ error|trans({}, 'FOSUserBundle') }}</div>
                {% endif %}
            </div>
            <div class="row admin-auth-form">
                <h1 class="admin-auth-title">Identifiez-vous</h1>
                <form action="{{ path("fos_user_security_check") }}" method="post">
                    <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
                    <div class="row">
                    <label for="username">Login :</label>
                    <input type="text" id="username" name="_username" value="{{ last_username }}" required="required" />
                    </div>
                    <div class="row">
                                <label for="password">{{ 'security.login.password'|trans({}, 'FOSUserBundle') }}</label>
                                <input type="password" id="password" name="_password" required="required" />
                    </div>
                    <div class="row">
                    <input type="checkbox" id="remember_me" name="_remember_me" value="on" />
                    <label for="remember_me" class="remember">{{ 'security.login.remember_me'|trans({}, 'FOSUserBundle') }}</label>
                    </div>
                    <div class="row">
                    <input type="submit" id="_submit" name="_submit" value="Se connecter" />
                    </div>
                </form>
            </div>
             
    </div>
    
</div>

    </body>
</html>

